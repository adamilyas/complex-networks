         \PY{k}{for} \PY{n}{network\PYZus{}path}\PY{p}{,} \PY{n}{network\PYZus{}name}\PY{p}{,} \PY{n}{network\PYZus{}title} \PY{o+ow}{in} \PY{n}{data}\PY{p}{:}
